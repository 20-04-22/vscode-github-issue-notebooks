{
	"name": "vscode-github-issue-notebooks",
	"displayName": "GitHub Issue Notebooks",
	"description": "GitHub Issue Notebooks for VS Code",
	"publisher": "vscode",
	"version": "0.0.1",
	"enableProposedApi": true,
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onLanguage:github-issues"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "github-issues",
				"aliases": [
					"GitHub Issues"
				],
				"filenamePatterns": [
					"*.github-issues.txt"
				]
			}
		],
		"grammars": [
			{
				"language": "github-issues",
				"scopeName": "source.github-issues",
				"path": "./syntaxes/github-issues.tmGrammar.json"
			}
		],
		"notebookProvider": [
			{
				"viewType": "github-issues",
				"displayName": "GitHub Issues Notebook",
				"selector": [
					{
						"filenamePattern": "*.github-issues"
					}
				]
			}
		],
		"commands": [
			{
				"title": "Clear Notebook Cell Output",
				"command": "github-issues.clearCellOutput",
				"icon": "$(clear-all)"
			},
			{
				"title": "Clear All Notebook Cells Output",
				"command": "github-issues.clearAllCellsOutput",
				"icon": "$(clear-all)"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "github-issues.clearAllCellsOutput",
					"group": "navigation",
					"when": "notebookEditorFocused"
				}
			],
			"notebook/cell/title": [
				{
					"command": "github-issues.clearCellOutput",
					"when": "notebookCellType == code"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint --config .eslintrc.json ./src/**/*.ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"unit-test": "npx mocha ./out/unit-test",
		"precommit": "npm run lint"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"husky": "^4.2.3",
		"mocha": "^7.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@octokit/rest": "^17.1.0"
	}
}